<div class="list-container">
  <h3 class="center white">Edit list</h3>
  <form action="/lists/<%= @list.id %>" method="post">
    <input type="hidden" name="_method" value="put">
    <%= erb :'lists/_list_form', locals: {list: @list} %>
    <button type="submit" class="btn btn-primary">Finish editing</button>
  </form>
</div>